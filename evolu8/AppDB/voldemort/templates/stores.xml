<stores>
  <store>
    <name>appscale</name>
    <persistence>bdb</persistence>
    <routing>client</routing>
    <replication-factor>REPLICATION</replication-factor>
    <required-reads>VOLDEMORT-R</required-reads>
    <required-writes>VOLDEMORT-W</required-writes>
    <key-serializer>
      <type>string</type>
      <schema-info>utf8</schema-info>
    </key-serializer>
    <value-serializer>
      <type>string</type>
      <schema-info>utf8</schema-info>
    </value-serializer>
  </store>
</stores>